algoritmo "lista_01"
var

   nome: caractere
   idade, altura:  real

inicio

      escreva("Digite seu nome: ")
      leia(nome)
      escreva("Digite sua idade: ")
      leia(idade)
      escreva("Digite sua altura: ")
      leia(altura)

      escreval("NOME: ", nome)
      escreval("IDADE:", idade)
      escreval("ALTURA: ", altura)

fimalgoritmo
